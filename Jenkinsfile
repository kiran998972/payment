pipeline {
  
  agent any
  
  stages {
  
    stage('Git clone') {
      steps {
        
        git branch:'main', url: 'https://github.com/mnagen/payment.git'
        echo "cloning the code from git repository"
      }}
      
      stage('Build') {
        
        steps {
        echo "Building the application"
        }
        
        
        
      }
    
    
    
  
  
  
  }
}
